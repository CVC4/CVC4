(set-logic QF_SLIA)
;(set-info :status unsat)
(declare-fun x () (Seq (Seq Int)))
(declare-fun xp () (Seq (Seq Int)))
(declare-fun y () (Seq (Seq Int)))
(declare-fun yp () (Seq (Seq Int)))
(declare-fun z () (Seq Int))
(declare-fun w () (Seq Int))
(declare-fun i () Int)
(declare-fun j () Int)
(declare-fun n () Int)
(assert (> i j))
(assert (= (str.substr z n 1) (seq.unit i)))
(assert (= (str.substr w n 1) (seq.unit j)))
(assert (= x (str.++ (seq.unit z) xp)))
(assert (= y (str.++ (seq.unit w) yp)))
(assert (= x y))
(check-sat)
