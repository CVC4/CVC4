% COMMAND-LINE: --incremental
U : TYPE;
x, y : U;
% EXPECT: invalid
QUERY x = y;
ASSERT x = y;
% EXPECT: entailed
QUERY x = y;
PUSH;
z : U;
% EXPECT: entailed
QUERY x = y;
% EXPECT: invalid
QUERY x = z;
% EXPECT: invalid
QUERY z = x;
% EXPECT: invalid
QUERY z /= x;
POP;
% EXPECT: invalid
QUERY z /= x;
